      {  
         "name":"cw-bind9",
         "vendor":"fokus",
         "version":"3.2.0",
         "lifecycle_event":[  
            {  
               "event":"CONFIGURE",
               "lifecycle_events":[
			"fhoss_relation_joined.sh",
			"bono_relation_joined.sh",
			"sprout_relation_joined.sh",
			"dime_relation_joined.sh",
			"homer_relation_joined.sh",
			"vellum_relation_joined.sh",
			"ellis_relation_joined.sh"
               ]
            },
            {  
               "event":"INSTANTIATE",
               "lifecycle_events":[ 
			"bind9_install.sh",
			"generate_zone_file.sh"
               ]
            },
            {
               "event":"START",
               "lifecycle_events":[
			"bind9_restart.sh"
               ]
            }
         ],
         "configurations":{  
            "name":"client-configuration",
            "configurationParameters":[
	       {  
                  "confKey":"realm",
                  "value":"example.com"
               },
	       {  
                  "confKey":"useFloatingIpsForEntries",
                  "value":"false"
               },
	       {  
                  "confKey":"downloadPackages",
                  "value":"true"
               }
            ]
         },
         "vdu":[  
            {  
               "vm_image":[],
               "scale_in_out":1,
               "vnfc":[  
                  {  
                     "connection_point":[  
                        {  
                           "virtual_link_reference":"mgmt"
                        } 
                     ]
                  }
               ]
            }
         ],
         "virtual_link":[  
            {  
               "name":"mgmt"
            }
         ],
         "deployment_flavour":[  
            {  
               "flavour_key":"m1.small"
            }
         ],
         "type":"bind9",
         "endpoint":"generic"
      }
